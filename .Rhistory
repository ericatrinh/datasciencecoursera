pollutantmean <- function(directory, pollutant, id = 1:332){
dir = c( getwd(), directory, "/" )
average <- 0
for(i in id){
monitor <- c( dir, toString(i), ".csv" )
table <- read.csv(monitor)
if( pollutant == "sulfate"){
data <- table[,2]
}
if( pollutant == "nitrate" ){
data <- table[,3]
}
average <- c(data[!is.na(table)], average)
}
mean(average)
}
